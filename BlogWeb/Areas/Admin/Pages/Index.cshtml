@page
@using BlogWeb.Areas.Admin.Pages;
@model IndexModel
@{
    ViewData["selected"] = "index";
}
<section class="content-header">
    <h1>
        پنل ادمین
        <small>آماده برای پروژه شما</small>
    </h1>
   
</section>
<section class="content">
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">داشبرد</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                        title="جمع شود">
                    <i class="fa fa-minus"></i>
                </button>
                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="حذف">
                    <i class="fa fa-times"></i>
                </button>
            </div>
        </div>
        <div class="box-body" style="background-color: cornsilk">
            به پنل ادمین خوش آمدید. چنانچه بعد از اعمال تغییرات به این صفحه بازگردانده شدید بدانید که آن تغییرات در صفحه اصلی شما اعمال شده است
        </div>
      
    </div>
</section>
